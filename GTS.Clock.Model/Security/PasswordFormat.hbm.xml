<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
  <class name="GTS.Clock.Model.Security.PasswordFormat, GTS.Clock.Model"
         table="TA_PasswordFormat" lazy="true">

    <id name="ID" access="property"  column="passwordFormat_ID" >
      <generator class="native"/>
    </id>

    <property name="ContainsWords" access="property" column="passwordFormat_ContainsWords" />
    <property name="ContainsNumbers" access="property" column="passwordFormat_ContainsNumbers"/>
    <property name="ContainsSymbols" access="property" column="passwordFormat_ContainsSymbols"/>
    <property name="MinLength" access="property" column="passwordFormat_MinLength"/>
    <property name="MaxLength" access="property" column="passwordFormat_MaxLength"/>


  </class>
</hibernate-mapping>
